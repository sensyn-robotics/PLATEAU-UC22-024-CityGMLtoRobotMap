{
    "metadata": [
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "PLATEAU CityGMLtoRobotMap",
            "_appFooter": "PLATEAU CityGMLtoRobotMap",
            "_enableSearch": true
        },
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
            {
                "src": "manual",
                "files": [
                    "toc.yml",
                    "*.md"
                ],
                "dest": "manual"
            }
        ],
        "resource": [
            {
                "files": [
                    "resources/**/*"
                ]
            }
        ],

        "xref": [
            "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "../.docs/_site"
    }
}